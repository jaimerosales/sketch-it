{
  "name": "sketch-it",
  "version": "1.0.0",
  "description": "Sketch It",
  "main": "server/generated/main.js",
  "scripts": {
    "start": "node server/generated/main.js",
    "postinstall": "npm run build",
    "clean": "git clean -fxd",
    "build": "npm run client-build && npm run server-build",
    "watch": "npm run client-watch &  npm run server-watch",
    "debug": "npm run watch & nodemon",
    "client-build": "browserify            -g browserify-css -t [ babelify --presets [ react env ] --plugins [ transform-class-properties transform-object-rest-spread ] ] www/entry/app.js -o www/generated/sketch_it_bundle.js",
    "client-watch": "watchify   --debug -v -g browserify-css -t [ babelify --presets [ react env ] --plugins [ transform-class-properties transform-object-rest-spread ] ] www/entry/app.js -o www/generated/sketch_it_bundle.js",
    "server-build": "babel         --presets env --plugins transform-class-properties server/src --out-dir server/generated",
    "server-watch": "babel --watch --presets env --plugins transform-class-properties server/src --out-dir server/generated"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "almost-equal": "1.1.0",
    "body-parser": "1.18.2",
    "bootstrap": "4.0.0",
    "classnames": "2.2.5",
    "debounce": "1.1.0",
    "ejs": "2.5.8",
    "es6-promisify": "6.0.0",
    "express": "4.16.3",
    "gl-matrix": "2.4.0",
    "jquery": "3.3.1",
    "popper.js": "1.14.3",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "request": "2.85.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "16.1.1",
    "browserify-css": "0.14.0"
  }
}
